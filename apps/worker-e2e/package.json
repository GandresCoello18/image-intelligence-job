{
  "name": "@org/worker-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "worker-e2e",
    "implicitDependencies": [
      "worker"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/worker-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "worker:build",
          "worker:serve"
        ]
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/worker-e2e/jest.config.cts",
          "passWithNoTests": true
        }
      }
    }
  }
}
