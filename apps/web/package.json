{
  "name": "@org/web",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "web",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "--node-env=production"
          ],
          "cwd": "apps/web"
        },
        "configurations": {
          "development": {
            "args": [
              "--node-env=development"
            ]
          }
        }
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "web:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "web:build:development"
          },
          "production": {
            "buildTarget": "web:build:production"
          }
        }
      }
    }
  },
  "dependencies": {
    "fastify": "~5.2.1",
    "fastify-plugin": "~5.0.1",
    "@fastify/view": "^11.1.1",
    "@fastify/static": "^9.0.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/multipart": "^9.3.0",
    "@fastify/sensible": "~6.0.2",
    "pug": "^3.0.3",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {}
}

